{
	"name": "tescall",
	"version": "1.2.4",
	"description": "Support ticket bot",
	"license": "MIT",
	"author": "Vare (https://vare.pw/)",
	"type": "module",
	"private": true,
	"scripts": {
		"start": "node --unhandled-rejections=warn --experimental-specifier-resolution=node .",
		"debug": "nodemon --unhandled-rejections=warn --experimental-specifier-resolution=node .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"engines": {
		"node": ">=16"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vare-team/tescall.git"
	},
	"bugs": {
		"url": "https://github.com/vare-team/tescall/issues"
	},
	"homepage": "https://github.com/vare-team/tescall#readme",
	"nodemonConfig": {
		"ignore": [
			"threads.json",
			"tickets.json",
			"mutes.json"
		]
	},
	"dependencies": {
		"discord.js": "^14.16.3",
		"ms": "^2.1.3"
	},
	"devDependencies": {
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.8",
		"erlpack": "github:discord/erlpack",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.9"
	}
